{

  "description": "Adds Chess Timer to HorsePaste",
  "manifest_version": 2,
  "name": "ChessTimer",
  "version": "1.0",
  "homepage_url": "https://github.com/tommybobo/horsepastechessclock",
  "icons": {
    "48": "icons/png-clipart-stopwatch-timer-time-bomb-angle-accessories.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.horsepaste.com/*"],
      "js": ["hpChess.js"]
    }
  ]

}
